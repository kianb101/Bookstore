@{
    ViewData["Title"] = "Home Page";
}

@model ProjectsListViewModel

    <h1>The Water Project</h1>

    @foreach (Project p in Model.Projects)
    {
        <h3>@p.ProjectName</h3>
        
        <ul>
            <li>Project Type: @p.ProjectType</li>
            <li>Regional Program: @p.ProgramName</li>
            <li>Project Name: @p.ProjectName</li>
            <li>Impact: @p.ProjectImpact Served</li>
            <li>Installation Date: @p.ProjectINstallation</li>

        </ul>
    }

<div page-model="@Model.PaginationInfo" page-action="Index"></div>
